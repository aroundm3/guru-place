{
  "kind": "collectionType",
  "collectionName": "customers",
  "info": {
    "singularName": "customer",
    "pluralName": "customers",
    "displayName": "Customer"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "full_name": {
      "type": "string",
      "required": true
    },
    "dob": {
      "type": "date"
    },
    "phone_number": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "address": {
      "type": "string",
      "required": false
    },
    "point": {
      "type": "biginteger"
    },
    "orders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order.order",
      "mappedBy": "customer"
    },
    "promotions": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::promotion.promotion",
      "inversedBy": "customers"
    }
  }
}
