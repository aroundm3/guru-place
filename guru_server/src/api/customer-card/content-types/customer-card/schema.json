{
  "kind": "collectionType",
  "collectionName": "customer_cards",
  "info": {
    "singularName": "customer-card",
    "pluralName": "customer-cards",
    "displayName": "CustomerCard"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "description": {
      "type": "text",
      "required": false
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "index": {
      "type": "biginteger",
      "required": true,
      "unique": true
    },
    "discount": {
      "type": "biginteger",
      "required": true,
      "default": "20000"
    },
    "products": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product.product",
      "inversedBy": "customer_cards"
    }
  }
}
